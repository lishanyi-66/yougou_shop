.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_cate {
  display: flex;
}
.index_cate navigator {
  flex: 1;
  padding: 20rpx;
}
.index_cate navigator image {
  width: 100%;
}
.index_floor .index_classGood .classGood_title {
  margin-top: 10rpx;
}
.index_floor .index_classGood .classGood_title image {
  width: 100%;
}
.index_floor .index_classGood .classGood_list {
  overflow: hidden;
  margin-top: 10rpx;
}
.index_floor .index_classGood .classGood_list navigator {
  box-sizing: border-box;
  float: left;
  width: 33.33%;
}
.index_floor .index_classGood .classGood_list navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.index_floor .index_classGood .classGood_list navigator:nth-child(2),
.index_floor .index_classGood .classGood_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .index_classGood .classGood_list navigator image {
  width: 100%;
